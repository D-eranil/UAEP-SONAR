    if ($(window).width() <= 767){
        if ($('.bootstrapModalFullCalendar').length > 0) {
            $('.bootstrapModalFullCalendar').fullCalendar({
                header: {
                    left: '',
                    center: 'prev title next',
                    right: ''
                },
				eventLimit: 2,
                eventClick: function (event, jsEvent, view) {
                    $('#modalTitle').html(event.title);
                    $('#modalBody').html(event.description);
                    $('#eventUrl').attr('href', event.url);
                    if (event.url == 'javascript:' || !event.url) {
                        $('#eventUrl').attr('target', "");
                    } else {
                        $('#eventUrl').attr('target', " ");
                    } 
                    $('.fullCalModal').modal();
					scroll.stop();
                    return false;

                },
                events: 'http://uaep.dev.local/GetAllEvents.aspx',

                eventRender: function (event, eventElement) {
                    if (event.imageurl) {
                        eventElement.find("div.fc-content").prepend("<img src='" + event.imageurl + "' width='' height=''>");
                    }
                },


            });
        }
    }
   
    if ($('.bootstrapModalFullCalendar').length > 0) {
        //getEventsListing();
        $('.bootstrapModalFullCalendar').fullCalendar({
            header: {
                left: '',
                center: 'prev title next',
                right: ''
            },
            eventClick: function (event, jsEvent, view) {
                $('#modalTitle').html(event.title);
                $('#modalBody').html(event.description);
                $('#eventUrl').attr('href', event.url);
                if (event.url == 'javascript:' || !event.url) {
                    $('#eventUrl').attr('target', "");
                } else {
                    $('#eventUrl').attr('target', " ");
                } 
                $('.fullCalModal').modal();
				scroll.stop();
                return false;

            },
            events: 'http://uaep.dev.local/GetAllEvents.aspx'
               ,
            columnFormat: {

                month: 'dddd',
            },
            eventRender: function (event, eventElement) {
                if (event.imageurl) {
                    eventElement.find("div.fc-content").prepend("<img src='" + event.imageurl + "' width='' height=''>");
                }
            },
        });

    }
	
