    function openURL() {
        var name = document.getElementById('textareaID').value;
        var url = 'http://uaep.dev.local/ar/search.aspx?searchtext=' + name + '&searchmode=exactphrase';

        // In current window
       //window.location = url;
        // window.open(url);
        window.location.replace(url);
    }

    $('#icon-search').on("click", function (e) {
        e.preventDefault();
        openURL();
    });

